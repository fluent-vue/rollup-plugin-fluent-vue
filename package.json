{
  "name": "rollup-plugin-fluent-vue",
  "version": "3.1.0-6",
  "description": "Rollup and Vite plugin for custom block support for fluent-vue",
  "author": "Ivan Demchuk <ivan.demchuk@gmail.com>",
  "license": "MIT",
  "homepage": "https://fluent-vue.demivan.me/integrations/rollup.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/Demivan/fluent-vue"
  },
  "keywords": [
    "localization",
    "l10n",
    "internationalization",
    "i18n",
    "ftl",
    "locale",
    "language",
    "formatting",
    "translate",
    "translation",
    "format",
    "vue",
    "vuejs",
    "vue.js",
    "rollup-plugin",
    "vite-plugin"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "magic-string": "^0.26.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "@types/node": "17.0.23",
    "@vitejs/plugin-vue": "3.0.0",
    "@vue/compiler-sfc": "3.2.37",
    "c8": "7.11.3",
    "eslint": "8.19.0",
    "release-it": "15.1.2",
    "rollup-plugin-vue": "6.0.0",
    "tsup": "6.1.3",
    "typescript": "*",
    "vite": "3.0.0",
    "vite-plugin-vue2": "2.0.2",
    "vitest": "0.18.0",
    "vue-template-compiler": "2.7.6"
  }
